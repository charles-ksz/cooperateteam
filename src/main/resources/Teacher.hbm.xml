<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dhu.pojo.Teacher" table="teacher" schema="test">
        <id name="id" column="id"/>
        <property name="goodSubject" column="goodSubject"/>
        <property name="freeTime" column="freeTime"/>
        <property name="fromStudentInformation" column="fromStudentInformation"/>
        <property name="state" column="state"/>
        <property name="honor" column="honor"/>
        <property name="remark" column="remark"/>
    </class>
</hibernate-mapping>