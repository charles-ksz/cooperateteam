<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dhu.pojo.Student" table="student" schema="test">
        <id name="id" column="id"/>
        <property name="badSubject" column="badSubject"/>
        <property name="freeTime" column="freeTime"/>
        <property name="fromTeacherInformation" column="fromTeacherInformation"/>
        <property name="remark" column="remark"/>
    </class>
</hibernate-mapping>